.container
  section
    .row
      .col-md-3
      .col-md-4
        .row
          .panel.panel-success
            .panel-heading Your Documents
            .panel-body(ng-repeat='each in document.documents | reverse')
              h3 {{ each.title }}
              p {{ each.content }}
              p Owner: {{ each.ownerId}}
              p Created: {{ each.dateCreated }}
      .col-md-3
